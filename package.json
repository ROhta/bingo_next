{
  "name": "bingo_next",
  "version": "0.1.0",
  "author": "ROhta",
  "description": "A Bingo Machine for Party",
  "private": false,
  "license": "GPL-3.0-or-later",
  "homepage": "https://rohta.github.io/bingo_next/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ROhta/bingo_next"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.3.0",
    "npm": ">=8.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 1000,
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "rangeStart": 0,
    "rangeEnd": 10000,
    "filepath": "none",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "ignore",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "auto"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "ignorePatterns": [
      "/node_modules/*",
      ".next/*"
    ]
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "recoil": "^0.7.6"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
