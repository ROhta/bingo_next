{
  "name": "bingo_next",
  "version": "0.1.0",
  "private": false,
  "author": "ROhta",
  "description": "A Bingo Machine for Party",
  "license": "GPL-3.0-or-later",
  "homepage": "https://rohta-bingo-next.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ROhta/bingo_next"
  },
  "bugs": {
    "url": "https://github.com/ROhta/bingo_next/security/policy"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "cd .. && husky install application/.husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,ts,tsx}": [
      "pnpm exec eslint . --fix",
      "pnpm exec prettier --write ."
    ]
  },
  "prettier": {
    "printWidth": 1000,
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "rangeStart": 0,
    "rangeEnd": 10000,
    "filepath": "none",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "ignore",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@storybook/addon-essentials": "^8.6.0",
    "@storybook/addon-interactions": "^8.6.0",
    "@storybook/addon-onboarding": "^8.6.0",
    "@storybook/blocks": "^8.6.0",
    "@storybook/nextjs": "^8.6.0",
    "@storybook/react": "^8.6.0",
    "@storybook/test": "^8.6.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.3",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.0",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "sharp"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
